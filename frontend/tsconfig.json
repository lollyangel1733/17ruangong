{
  "compilerOptions": {
    "module": "esnext",
    "target": "ES2020",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#app": [".nuxt/types/app"],
      "#imports": [".nuxt/types/imports"],
      "nitropack": ["./node_modules/nitropack"],
      "vue": ["./node_modules/vue"]
    },
    "types": ["@types/node", "vue"],
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["./**/*.ts", "./**/*.vue", ".nuxt/types/**/*.d.ts", ".nuxt/**/*.d.ts"],
  "files": ["./global.d.ts"],
  "vueCompilerOptions": {
    "globalTypesPath": "./.nuxt/nuxt.d.ts"
  }
}
